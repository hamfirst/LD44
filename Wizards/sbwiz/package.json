{
    "name": "sbwiz",
    "displayName": "sbwiz",
    "description": "StormBrewer Engine Wizards",
    "version": "0.0.1",
    "publisher": "StormBrewersLLC",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:StormBrewerEngine.cmake"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.addSBECode",
                "title": "AddCode",
                "category": "StormBrewers"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.addSBECode",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
